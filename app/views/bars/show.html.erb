<div id="results"><br><br><br>
  <h2 style="text-align:center"><%= @bar.name %></h2>
  <h3 style="text-align:center"><%= @bar.address %></h3>
  <div class="bs-component"><br>
    <table class ="table table-striped table-hover">
      <tr>
        <th>Check Ins</th>
        <td><%= @bar.checkinsCount %></td>
      </tr>
      <tr>
        <th>Unique User Checkins</th>
        <td><%= @bar.usersCount %></td>
      </tr>
      <tr>
        <th>Here Now</th>
        <td><%= @bar.here_now %></td>
      </tr>
    </table>
  </div>  
</div>

<div id="photo-box">
  <ul id="gallery-nav">
    <li>
      <% @photos.each do |photo| %>
        <a class="inline" href="#" ><img src= <%= photo %> alt=""></a>
      <% end %>
    </li>
  </ul>
</div>




   

